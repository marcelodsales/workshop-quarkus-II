			"item": [
				{
					"name": "Create Account - Valid Request",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"pm.test(\"Response has correct content type\", function () {",
									"    pm.response.to.have.header(\"Content-Type\");",
									"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");",
									"});",
									"",
									"pm.test(\"Response body has required fields\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property(\"accountNumber\");",
									"    pm.expect(jsonData).to.have.property(\"balance\");",
									"    pm.expect(jsonData).to.have.property(\"ownerId\");",
									"});",
									"",
									"pm.test(\"Account number matches request\", function () {",
									"    var jsonData = pm.response.json();",
									"    var requestData = JSON.parse(pm.request.body.raw);",
									"    pm.expect(jsonData.accountNumber).to.equal(requestData.accountNumber);",
									"});",
									"",
									"pm.test(\"Initial balance is correct\", function () {",
									"    var jsonData = pm.response.json();",
									"    var requestData = JSON.parse(pm.request.body.raw);",
									"    pm.expect(jsonData.balance).to.equal(requestData.initialBalance);",
									"});",
									"",
									"pm.test(\"Owner ID matches request\", function () {",
									"    var jsonData = pm.response.json();",
									"    var requestData = JSON.parse(pm.request.body.raw);",
									"    pm.expect(jsonData.ownerId).to.equal(requestData.ownerId);",
									"});",
									"",
									"pm.environment.set(\"accountNumber1\", pm.response.json().accountNumber);"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
